{
    "plugins": ["prettier-plugin-jinja-template"],
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 80,
    "overrides": [
        {
            "files": "*.njk",
            "options": {
                "parser": "jinja-template"
            }
        }
    ]
}
